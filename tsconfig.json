{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strictNullChecks": true, 
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#config/*":["./src/config/*"],
      "#middleware/*":["./src/middleware/*"]
    }
  },
  
}
